{
  "name": "frontoffice",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@livechat/ui-kit": "0.1.0",
    "charol": "^0.3.0",
    "i18next": "^11.9.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "manuh": "^1.4.3",
    "manuh-bridge-mqtt": "^0.0.6",
    "moment": "^2.22.2",
    "mqtt": "^2.18.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-i18next": "^8.0.7",
    "react-scripts": "1.1.5",
    "rhelena": "^1.2.5",
    "simple-mqtt-client": "^1.0.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "dev": "DEBUG=debug-chatServices,mqtt-provider-debug,mqttClient,debug-model-chatStation BACKEND_ENDPOINT=http://localhost:3000 AUTHENTICATION_ENDPOINT=http://localhost:3333 STATISTICS_ENDPOINT=http://localhost:8000 MQTT_BROKER_HOST=http://localhost:8080/mqtt MQTT_USERNAME=root MQTT_PASSWORD=root MAIN_PAGE_TITLE='Chimp Assist Demo' LOGIN_PAGE_TITLE='Login to Chimp Assist Demo' yarn start",
    "start": "REACT_APP_DEBUG=$DEBUG REACT_APP_BACKEND_ENDPOINT=$BACKEND_ENDPOINT REACT_APP_AUTHENTICATION_ENDPOINT=$AUTHENTICATION_ENDPOINT REACT_APP_STATISTICS_ENDPOINT=$STATISTICS_ENDPOINT REACT_APP_FORCE_i18n_LANGUAGE=$FORCE_i18n_LANGUAGE REACT_APP_MQTT_BROKER_HOST=$MQTT_BROKER_HOST REACT_APP_MQTT_USERNAME=$MQTT_USERNAME REACT_APP_MQTT_USERNAME=$MQTT_USERNAME REACT_APP_MQTT_BASE_TOPIC=chimpassist/demo REACT_APP_MAIN_PAGE_TITLE=$MAIN_PAGE_TITLE REACT_APP_LOGIN_PAGE_TITLE=$LOGIN_PAGE_TITLE react-scripts start",
    "build": "REACT_APP_DEBUG=$DEBUG REACT_APP_LOGO_SMALL_URL=$BUILD_LOGO_SMALL_URL REACT_APP_DEFAULT_ATTENDANT_AVATAR_URL=$BUILD_DEFAULT_ATTENDANT_AVATAR_URL REACT_APP_BACKEND_ENDPOINT=$BUILD_BACKEND_ENDPOINT REACT_APP_AUTHENTICATION_ENDPOINT=$BUILD_AUTHENTICATION_ENDPOINT REACT_APP_FORCE_i18n_LANGUAGE=$BUILD_FORCE_i18n_LANGUAGE REACT_APP_STATISTICS_ENDPOINT=$BUILD_STATISTICS_ENDPOINT REACT_APP_ONLINE_CUSTOMERS_LABEL=$BUILD_ONLINE_CUSTOMERS_LABEL REACT_APP_OFFLINE_CUSTOMERS_LABEL=$BUILD_OFFLINE_CUSTOMERS_LABEL REACT_APP_MQTT_BROKER_HOST=$BUILD_MQTT_BROKER_HOST REACT_APP_MQTT_USERNAME=$BUILD_MQTT_USERNAME REACT_APP_MQTT_PASSWORD=$BUILD_MQTT_PASSWORD  REACT_APP_MQTT_BASE_TOPIC=chimpassist/demo REACT_APP_MAIN_PAGE_TITLE=$BUILD_MAIN_PAGE_TITLE REACT_APP_LOGIN_PAGE_TITLE=$BUILD_LOGIN_PAGE_TITLE react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "jest-fetch-mock": "^1.6.6"
  }
}
